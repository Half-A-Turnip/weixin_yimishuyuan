/* baoming.wxss */
.bg{
   background-image:url("data:image/jpeg;base64,");
    background-repeat:no-repeat;
    background-size:cover;
    width:100%;
    height:100%;
    position:fixed;
    top:0;
    bottom:0;
    display: flex;
    flex-direction: column;
    justify-content:center;
}
.avatar{
    text-align: center;
    margin-top:40px;
    flex:0 0 60px;
}
.avatar .img{
    width: 60px;
    height: 60px;
}
.line{
    margin-top:10px;
    flex:0 0 20px;
    line-height: 20px;
    color: #fff;
    font-size:14px;
    display: flex;
    box-sizing: border-box;
    padding: 0 20px;
}
.line-before{
    flex: 1;
    position: relative;
    top: -2px;
    border-bottom: 1px solid rgba(255,255,255,0.6);
}
.line-content{
    padding: 0 12px;
    font-size: 16px;
}
.line-after{
    flex: 1;
    position: relative;
    top: -2px;
     border-bottom: 1px solid rgba(255,255,255,0.6);
}
.form{
    margin-top: 40px;
    text-align: center;
    flex:1;
    color: #fff;
}
.formgroup{
    box-sizing: border-box;
    padding: 0 20px;
    position: relative;
}
.groupSpan{
    width:100%;
    box-sizing:border-box;
    font-size:13px;
    padding:0 10px;
    margin-bottom:20px;
    border:1px solid #999;
    border-radius:5px;
    color:#fff;
    height:34px;
    line-height:34px;
    text-align: left;
}
.formgroupsmall .groupSpan{
    width:45%;
    display:inline-block;
}
.formgroupsmall .groupSpan:first-child{
    margin-right:5%;
}
.formgroupsmall .groupSpan:last-child{
    margin-left:5%;
}
.formgroup .btn{
    color: #fff;
    background: #00b38a;
    font-size:13px;
    height:34px;
    line-height:34px;
    margin-top: 30px;
}
.btn-small{
    position: absolute;
    /* background: rgba(255,255,255,0.3); */
    background: transparent;
    top: 0;
    color: #fff;
    right: 20px;
    font-size:13px;
    z-index:9;
}
.tetx{
    height:32px;
    line-height:32px;
}